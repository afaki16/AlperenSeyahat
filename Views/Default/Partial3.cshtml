@using TravelTripProje.Models.Classes
@model List<Blog>


<div class="box-left">
    @foreach (var m in Model)
    {
        <div class="bot-img">
            <img src="@m.BlogImage" alt="" />
        </div>
        <div class="bot-desc">
            <p>@m.Aciklama.Substring(0,80)</p>
        </div>
        <div class="clear"></div>
    }
</div>

@Html.Action("Partial4","Default")